{
  "name": "image-uploader-master",
  "version": "1.0.0",
  "scripts": {
    "client": "cd client && npm start",
    "start": "node server.js",
    "dev": "concurrently --kill-others-on-fail \"npm start\" \"npm client\"",
    "prod": "npm install && cd client && npm install && npm run build && cd ../ && npm start",
    "heroku-postbuild": "cd client && yarn install && yarn install --only=dev --no-shrinkwrap && yarn build"
  },
  "description": "DevChallenges full-stack challenge",
  "main": "server.js",
  "repository": "git@github.com:nb341/Image-Uploader.git",
  "author": "Narindra Balkissoon",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "multer": "^1.4.2"
  },
  "devDependencies": {
    "concurrently": "^6.0.0",
    "nodemon": "^2.0.7"
  }
}
